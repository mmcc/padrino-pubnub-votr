.block
  .secondary-navigation
    %ul.wat-cf
      %li.first=link_to pat(:list), url(:rounds, :index)
      %li.active=link_to pat(:new), url(:rounds, :new)
  .content
    %h2.title
      =pat(:new)
      =mt(:round)
    .inner
      -form_for :round, url(:rounds, :create), :class => :form do |f|
        =partial "rounds/form", :locals => { :f => f }

-#content_for :sidebar, partial("base/sidebar")
