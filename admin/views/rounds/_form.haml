- unless @last.blank?
  - content_for :js do
    %script
      :plain


.group
  =f.label :start_time
  =f.error_message_on :start_time
  =f.text_field :start_time, :class => "start_time text_field"
  %span.description If the last round isn't over, default is that round's end time. Otherwise it's the current time.

.group
  =f.label :end_time
  =f.error_message_on :end_time
  =f.text_field :end_time, :class => :text_field
  %span.description Default is 1 hour after the default start time.


.group.navform.wat-cf
  =f.submit pat(:save), :class => :button
  =f.submit pat(:cancel), :onclick => "window.location='#{url(:rounds, :index)}';return false", :class => :button
